Start = "fun" Patt "->" Expr

Exprs = (Expr #"\n")* Expr?

Expr =
  | constant
  | "fun" Patt "->" Expr -- fun ; function
  | "(" [contents]Expr ")" -- parens
  | [left]Expr "+" [right]Expr -- add

Patt = ident

constant =
  | ident
  | int64

ident = alpha+
int64 =  digit+

reserved =
  | "fun"
  | "let"

alpha = 'a..z'
digit = '0..9'
